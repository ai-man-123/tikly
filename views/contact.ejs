    <div class="welcome center">
      <div class="container">
        <div style="padding: 20px 0 20px 0" class="row">
          <div class="col s12">
            <h1 class="headone white-text">Contact Us - <%= titleweb %></h1>
          </div>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row section">
        <div
          class="col m10 offset-m1 z-depth-1 s12"
          style="border-top: 5px solid #ef124e"
        >
          <p class="center-align">
            If you have any questions or would like to report errors, please
            contact us below.
          </p>
          <div class="divider"></div>
          <div class="row">
            <form
              class="col m8 offset-m2 s12"
              action="/contact"
              method="post"
            >
              <div class="row">
                <div class="input-field col s12">
                  <i class="material-icons prefix">account_circle</i
                  ><input id="name" name="name" type="text" required />
                  <label for="name">Name</label>
                </div>
                <div class="input-field col s12">
                  <i class="material-icons prefix">email</i
                  ><input
                    id="email"
                    name="email"
                    type="email"
                    class="form-input"
                    required
                  />
                  <label for="email">Email</label>
                </div>
                <div class="input-field col s12">
                  <i class="material-icons prefix">edit</i
                  ><textarea
                    id="message"
                    name="message"
                    class="materialize-textarea"
                    required
                  ></textarea
                  ><label for="message">Message</label>
                </div>
              </div>
              <div class="row">
                <div class="col m12">
                  <p class="center-align">
                    <button
                      class="btn btn-large waves-effect waves-light orange"
                      type="submit"
                    >
                      <i class="material-icons left">send</i> Send Message
                    </button>
                  </p>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
